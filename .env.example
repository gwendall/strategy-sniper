# WebSocket RPC URL (required)
RPC_WSS=wss://your-ws-rpc-endpoint

# Private key for wallet (required)
PRIVATE_KEY=0x...

# Factory contract address (required)
FACTORY_ADDRESS=0x...

# Router contract address for Uniswap V4 (required)
ROUTER_ADDRESS=0x...

# Hook address override (optional - if not set, will read from factory.hookAddress())
HOOK_ADDRESS=0x...

# Amount of ETH to use for swaps (default: 0.05)
ETH_AMOUNT_IN=0.05